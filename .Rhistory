prettytable <- function(m, sigdig=4) {
mclass <- sapply(m, class)
nc <- mclass %in% c("numeric", "integer")
if(sum(nc) > 0 ) {
for(i in (1:dim(m)[2])[nc]) {
m[, nc] <- signif(m[, nc], sigdig)
}
}
m
}
